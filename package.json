{
  "name": "web-v4",
  "version": "0.0.0",
  "config": {
    "max_old_space_size": 4096
  },
  "scripts": {
    "prebuild": "setup-ng-alias",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy/proxy.lex-dev.js -o",
    "start:local": "ng serve --proxy-config proxy/proxy.local.js -o",
    "start:stage": "ng serve --proxy-config proxy/proxy.lex-stage.js -o",
    "start:siemens": "ng serve --proxy-config proxy/proxy.siemens.js -o",
    "build": "npm run build:en",
    "build:all": "npm run build:ar && npm run build:de && npm run build:en && npm run build:es && npm run build:fr && npm run build:ja && npm run build:nl && npm run build:zh_CN",
    "build:ar": "ng-alias build --prod --progress --baseHref=/ar/ --outputPath=dist/www/ar --i18nLocale=ar --i18nFile=src/locale/messages.ar.xlf",
    "build:de": "ng-alias build --prod --progress --baseHref=/de/ --outputPath=dist/www/de --i18nLocale=de --i18nFile=src/locale/messages.de.xlf",
    "build:en": "ng-alias build --prod --progress --baseHref=/ --outputPath=dist/www/en --i18nLocale=en",
    "build:es": "ng-alias build --prod --progress --baseHref=/es/ --outputPath=dist/www/es --i18nLocale=es --i18nFile=src/locale/messages.es.xlf",
    "build:fr": "ng-alias build --prod --progress --baseHref=/fr/ --outputPath=dist/www/fr --i18nLocale=fr --i18nFile=src/locale/messages.fr.xlf",
    "build:ja": "ng-alias build --prod --progress --baseHref=/ja/ --outputPath=dist/www/ja --i18nLocale=ja --i18nFile=src/locale/messages.ja.xlf",
    "build:nl": "ng-alias build --prod --progress --baseHref=/nl/ --outputPath=dist/www/nl --i18nLocale=nl --i18nFile=src/locale/messages.nl.xlf",
    "build:zh_CN": "ng-alias build --prod --progress --baseHref=/zh_CN/ --outputPath=dist/www/zh_CN --i18nLocale=zh_CN --i18nFile=src/locale/messages.zh_CN.xlf",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix=true",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/http": "^7.2.13",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/pwa": "^0.13.8",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "~8.0.0",
    "angular-calendar": "^0.27.4",
    "angular-cli-alias": "^1.0.9",
    "angular-draggable-droppable": "^4.1.0",
    "brace": "^0.11.1",
    "chart.js": "^2.8.0",
    "core-js": "^2.5.4",
    "d3": "^5.9.2",
    "date-fns": "^1.30.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "jsplumb": "^2.9.0",
    "keycloak-angular": "^6.1.0",
    "material-icons": "^0.3.1",
    "ng2": "^0.2.4",
    "ng2-ace-editor": "^0.3.9",
    "ng2-charts": "^2.2.2",
    "ng2-nvd3": "^2.0.0",
    "ngx-quill": "^4.8.0",
    "nodelist-foreach-polyfill": "^1.2.0",
    "quill": "^1.3.6",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "typeface-roboto": "0.0.54",
    "video.js": "^7.4.1",
    "videojs-contrib-quality-levels": "^2.0.9",
    "videojs-hls-quality-selector": "^1.0.5",
    "videojs-vr": "^1.5.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.1",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/chart.js": "^2.7.51",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/video.js": "^7.2.10",
    "@types/youtube": "0.0.36",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
